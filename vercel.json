{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/index.html"
    }
  ],
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "devCommand": "npm run dev",
        "buildCommand": "npm run build"
      }
    },
    {
      "src": "backend/package.json",
      "use": "@vercel/node"
    }
  ]
}


  